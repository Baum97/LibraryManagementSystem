               <font>
                  <Font size="29.0" />
               </font>
            </Label>
            <Button fx:id="returnBookButton" layoutX="277.0" layoutY="381.0" mnemonicParsing="false" onAction="#returnBookButton" prefHeight="35.0" prefWidth="113.0" text="Return book" />
